{
    "name": "c5vargas/laravel-api-server",
    "description": "A collection of Laravel API starter kits using the repository pattern. Very flexible and useful when developing medium to large scale applications.",
    "type": "project",
    "authors": [
        {
            "name": "Carles Vargas",
            "email": "carlesvargas5@gmail.com"
        }
    ],
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "^2.8",        
        "guzzlehttp/psr7": "2.4.5",
        "laravel/passport": "^11",
        "league/fractal": "^0.20.1",
        "doctrine/dbal": "^3.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "spatie/laravel-ignition": "^2.0",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
